    {score:200,feedback:"<b>Tremendous</b> correct = 8"},//image:"good.jpg"},
    {score:225,feedback:"<b>Gorgious</b> correct = 9"},//image:"good.jpg"},
    {score:250,feedback:"<b>Intelligent person in world</b> correct = 10"}//image:"good.jpg"}
      ];

	//"<br /><img src='" + grading[i].image + " width='300'  />";  
for(i=0; i<grading.length; i++) {
    if(result == grading[i].score) {
    result2 = grading[i].feedback +"<br/>Answers are <b>1.a,2.a,3.b,4.d,5.d,6.c,7.a,8.b,9.d,10.b</b>"//+"<br /><img url='"https:c0.wallpaperflare.com/preview/352/355/664/question-mark-knowledge-question-sign.jpg +" width='300'/>;
      
